<h1 class="pb-4">Explore Latest</h1>
<nav aria-label="breadcrumb" class="pb-4">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/">Home</a></li>
        <li class="breadcrumb-item active" aria-current="page">Explore-Latest</li>
    </ol>
</nav>
<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
    <% if (typeof recipes != 'undefined' && recipes.length >0) { %>
      <% recipes.forEach(recipe => { %>
        <a href="/recipe/<%= recipe._id %>"class="col text-center category__link">
          <div class="category__image category__image--large shadow">
            <img src="/upload/<%= recipe.image %>" alt="<%= recipe.name %>">
          </div>
          <div class="pt-1"><%= recipe.name %></div>
        </a>
      <% }) %>
    <% }  else { %>
      <p>Not item found</p>
    <% }  %>
</div>