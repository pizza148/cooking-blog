<h1 class="pb-4">Explore Random</h1>
<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
    <% if (typeof recipes != 'undefine') { %>
        <a href="/recipe/<%= recipes._id %>"class="col text-center category__link">
          <div class="category__image category__image--large shadow">
            <img src="/upload/<%= recipes.image %>" alt="<%= recipes.name %>">
          </div>
          <div class="pt-1"><%= recipes.name %></div>
        </a>
    <% }  else { %>
      <p>Not item found</p>
    <% }  %>
  
</div>